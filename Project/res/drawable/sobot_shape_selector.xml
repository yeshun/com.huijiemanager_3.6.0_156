<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff0cb273" android:endColor="#ff0cb273" android:angle="90.0" />
            <stroke android:width="2.0dip" android:color="#ff0cb273" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="@color/sobot_btn_send_selector_color" android:endColor="@color/sobot_btn_send_selector_color" android:angle="90.0" />
            <stroke android:width="2.0dip" android:color="@color/sobot_btn_send_selector_color" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>